from preloaded import Node

'''
Node is defined in preloaded like this:

class Node:
    def __init__(self, data):
        self.data = data
        self.next = None
'''
    
def push(head, data):
    a = Node(data)
    a.next = head
    return a
    # Your code goes here.
   # return push(head, None)


