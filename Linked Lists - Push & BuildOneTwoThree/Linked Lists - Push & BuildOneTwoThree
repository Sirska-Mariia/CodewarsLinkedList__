def build_one_two_three():
    # Your code goes here.
    chained = None
    chained = push(chained, 3)
    chained = push(chained, 2)
   # chained = push(chained, 1)
    k = Node(None)
    k = push(chained, 3)
    k = push(chained, 2)
    k = push(chained, 1)
    return k
