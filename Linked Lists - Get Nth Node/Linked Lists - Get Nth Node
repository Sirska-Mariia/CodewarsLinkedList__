def get_nth(node, index):
    if index < 0:
        raise Indexerror
    for i in range(index+1):
        if i == index:
            print(i, index, node.next, node.data)
            return Node(node.data, None)
        node = node.next
     #   node = node.data
       # node = node.next
    return Node(index)
